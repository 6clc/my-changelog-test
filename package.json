{
  "name": "my-changelog-test",
  "version": "1.1.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copy": "rimraf dist && gulp copy",
    "build": "npm run copy && tsc",
    "dev": "set NODE_ENV=dev&& nodemon ./index",
    "lint": "eslint src --fix --ext .js,.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git add . && git cz && git push",
    "cglog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.js",
    "rm": "rimraf node_modules",
    "watch": "npm run copy && tsc -w",
    "ver": "standard-version",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/my-changelog-test.git"
  },
  "author": "CaoMeiYouRen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/my-changelog-test/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/my-changelog-test#readme",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "compare-func": "^1.3.2",
    "conventional-changelog-angular-emoji": "^0.2.2",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-changelog-config-spec": "^2.1.0",
    "conventional-changelog-custom-config": "^0.2.0",
    "conventional-github-releaser": "^3.1.3",
    "conventional-recommended-bump": "^6.0.5",
    "cz-conventional-changelog": "^3.0.2",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^6.7.1",
    "gulp": "^4.0.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "release-it": "^12.4.3",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    ".{ts,js,vue}": [
      "npm run lint",
      "git add ."
    ]
  },
  "changelog": {
    "emojis": true,
    "authorName": false,
    "authorEmail": false
  }
}