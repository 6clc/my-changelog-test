{
  "name": "my-changelog-test",
  "version": "1.4.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "copy": "rimraf dist",
    "build": "npm run copy && tsc",
    "dev": "set NODE_ENV=dev&& nodemon ./index",
    "lint": "eslint src --fix --ext .js,.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commit": "git add . && git cz && git push",
    "changelog": "conventional-changelog -p cmyr-config -i CHANGELOG.md -s -r 0",
    "rm": "rimraf node_modules",
    "watch": "npm run copy && tsc -w",
    "ver": "standard-version",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/my-changelog-test.git"
  },
  "author": "CaoMeiYouRen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CaoMeiYouRen/my-changelog-test/issues"
  },
  "homepage": "https://github.com/CaoMeiYouRen/my-changelog-test#readme",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@types/download": "^6.2.4",
    "@types/fs-extra": "^8.0.1",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.7.1",
    "@types/request": "^2.48.3",
    "@types/tunnel": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "commitizen": "^4.0.3",
    "commitlint": "^8.2.0",
    "compare-func": "^1.3.2",
    "conventional-changelog-cli": "^2.0.28",
    "conventional-changelog-cmyr-config": "^1.0.0",
    "conventional-changelog-config-spec": "^2.1.0",
    "conventional-changelog-custom-config": "^0.2.0",
    "conventional-github-releaser": "^3.1.3",
    "conventional-recommended-bump": "^6.0.5",
    "cz-conventional-changelog": "^3.0.2",
    "dateformat": "^3.0.3",
    "eslint": "^6.7.1",
    "gulp": "^4.0.2",
    "husky": "^3.1.0",
    "install": "^0.13.0",
    "lint-staged": "^9.5.0",
    "npm": "^6.13.1",
    "release-it": "^12.4.3",
    "rimraf": "^3.0.0",
    "semantic-release": "^17.0.3",
    "semantic-release-gitmoji": "^1.3.3",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    ".{ts,js,vue}": [
      "npm run lint",
      "git add ."
    ]
  },
  "changelog": {
    "emojis": true,
    "authorName": false,
    "authorEmail": false
  },
  "dependencies": {
    "axios": "^0.19.0",
    "download": "^7.1.0",
    "fs-extra": "^8.1.0",
    "got": "^9.6.0",
    "md5": "^2.2.1",
    "request": "^2.88.0",
    "tunnel": "^0.0.6"
  }
}
